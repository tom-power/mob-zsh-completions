#compdef mob

$mobLib

_arguments \
  '1:cmd:_commands' \
  '*::args:->args'

if [[ $state == 'cmd' ]]; then
  _cmdItems
fi

if [[ $state == 'args' ]]; then
  case $words[$CURRENT-1] in
    '-b' )
      _branchArgValues ;;
    *)
    case $words[1] in
      'start' )
        _startArgs ;;
      'next' )
        _nextArgs ;;
      'done' )
        _doneArgs ;;
      'reset' )
        _resetArgs ;;
      *)
    esac
  esac
fi
